    "ges-client": "^0.9.1",
    "invariant": "^2.1.0",
    "lodash": "^3.9.3",
    "rx": "^2.5.3",
    "uuid": "^2.0.1",
    "DAGon":"0.0.1"
  },
  "devDependencies": {
    "babel": "^5.5.6",
    "babel-runtime": "^5.5.6",
    "babelify": "^5.0.4",
    "gulp": "3.9.x",
    "gulp-concat": "2.5.x",
    "gulp-if": "^1.2.5",
    "gulp-less": "3.0.x",
    "gulp-minify-css": "0.4.x",
    "gulp-nodemon": "1.0.x",
    "gulp-sourcemaps": "1.3.x",
